spring.application.name=AgendaManager

# Server configuration
server.port=9002

# spring.jpa.hibernate.ddl-auto: specifies the strategy for creating and dropping database tables
spring.datasource.url=jdbc:postgresql://localhost:5432/P2PM
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true

# spring.datasource.username: specifies the username for the PostgreSQL database connection
spring.datasource.username = postgres
spring.datasource.password = abcd1234